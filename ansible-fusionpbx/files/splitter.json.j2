		"clientname": "DUMMY CLOSING ENTRY for Uniloader {{ uniloader_version}}",
		"uri": "file:disabled-closing-entry.txt",
		"login": "",
		"pass": "",
		"token": "",
		"matcher": [""],
		"match": "any",
		"removematch": true,
		"disabled": true,
		"noactions": true
	}

]


